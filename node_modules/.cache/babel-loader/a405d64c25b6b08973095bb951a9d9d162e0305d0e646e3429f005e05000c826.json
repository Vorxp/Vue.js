il grade_center\">\r\n              <span>{{ subj.Credit }} หน่วยกิต</span>\r\n            </div>\r\n\r\n            <div class=\"col-4 col-md-3 border_grade\">\r\n              <p class=\"grade_Text_600\" style=\"margin-bottom: 0px\">\r\n                {{ subj.SubjNameTh }}\r\n              </p>\r\n              <span class=\"grade_subject\"> {{ subj.SubjName }} </span>\r\n            </div>\r\n\r\n            <div class=\"col-4 col-md-3 grade_year\">\r\n              <p class=\"grade_Text_600\" style=\"margin-bottom: 0px\">\r\n                ปีการศึกษา {{ subj.year }}\r\n              </p>\r\n              <span class=\"grade_yr\"> ภาค{{ subj.Semester }} </span>\r\n            </div>\r\n\r\n            <div class=\"col-4 col-md-2 grade_char\">\r\n              <span> {{ subj.Grade }} </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n<SubjDel :subId=\"subj.SubjID\" @edit=\"showEdit(subj.SubjID)\" @delete=\"deleteSubject(subj.SubjID)\" />\r\n  <div v-if=\"isEdit\">\r\n    <div class=\"alert alert-danger mt-3\"> Edit {{ editId }}</div>\r\n  </div>\r\n    </li>\r\n  </div>\r\n  \r\n</template>\r\n\r\n<script>\r\nimport SubjDel from \"./SubjDel.vue\";\r\n\r\nexport default {\r\n  name: \"SubjList\",\r\n  components: {\r\n    SubjDel,\r\n  },\r\n  data() {\r\n    return {\r\n      Subject: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    fetch(\"http://localhost:3000/course\")\r\n      .then((res) => res.json())\r\n      .then((data) => (this.Subject = data))\r\n      .catch((err) => console.log(err.message));\r\n  },\r\n  methods: {\r\n    showEdit(theSubjID) {\r\n      this.editId = theSubjID;\r\n      this.isEdit = true;\r\n    },\r\n    reload(){\r\n      this.$parent.showStdList =!this.$parent.showStdList\r\n    },\r\n    deleteSubject(SubjID) {\r\n    fetch(`http://localhost:3000/course/${SubjID}`, {\r\n      method: 'DELETE'\r\n    })\r\n    .then(response => {\r\n      if (!response.ok) {\r\n        throw new Error('Network response was not ok');\r\n      }\r\n      // ลบข้อมูลที่ถูกลบออกจาก array ใน Vue instance\r\n      this.Subject = this.Subject.filter(subj => subj.SubjID !== SubjID);\r\n    })\r\n    .catch(error => {\r\n      console.error('There was a problem with your fetch operation:', error);\r\n    });\r\n  }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"mappings":"AA4CA,OAAOA,OAAM,MAAO,eAAe;AAEnC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRC,KAAK,CAAC,8BAA8B,EACjCC,IAAI,CAAEC,GAAG,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACxBF,IAAI,CAAEJ,IAAI,IAAM,IAAI,CAACC,OAAM,GAAID,IAAK,EACpCO,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,OAAO,CAAC,CAAC;EAC7C,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAACC,SAAS,EAAE;MAClB,IAAI,CAACC,MAAK,GAAID,SAAS;MACvB,IAAI,CAACE,MAAK,GAAI,IAAI;IACpB,CAAC;IACDC,MAAMA,CAAA,EAAE;MACN,IAAI,CAACC,OAAO,CAACC,WAAU,GAAG,CAAC,IAAI,CAACD,OAAO,CAACC,WAAU;IACpD,CAAC;IACDC,aAAaA,CAACC,MAAM,EAAE;MACtBlB,KAAK,CAAE,gCAA+BkB,MAAO,EAAC,EAAE;QAC9CC,MAAM,EAAE;MACV,CAAC,EACAlB,IAAI,CAACmB,QAAO,IAAK;QAChB,IAAI,CAACA,QAAQ,CAACC,EAAE,EAAE;UAChB,MAAM,IAAIC,KAAK,CAAC,6BAA6B,CAAC;QAChD;QACA;QACA,IAAI,CAACxB,OAAM,GAAI,IAAI,CAACA,OAAO,CAACyB,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACN,MAAK,KAAMA,MAAM,CAAC;MACpE,CAAC,EACAd,KAAK,CAACqB,KAAI,IAAK;QACdnB,OAAO,CAACmB,KAAK,CAAC,gDAAgD,EAAEA,KAAK,CAAC;MACxE,CAAC,CAAC;IACJ;EACA;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}